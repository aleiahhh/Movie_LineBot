digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	user [label="user\l" color=red fillcolor=darksalmon peripheries=2]
	menu [label="menu\l"]
	now_playing [label="now_playing\l"]
	popular [label="popular\l"]
	top_rated [label="top_rated\l"]
	upcoming [label="upcoming\l"]
	overview [label="overview\l"]
	user -> menu [label="advance [is_going_to_menu]"]
	menu -> now_playing [label="advance [is_going_to_now_playing]"]
	menu -> popular [label="advance [is_going_to_popular]"]
	menu -> top_rated [label="advance [is_going_to_top_rated]"]
	menu -> upcoming [label="advance [is_going_to_upcoming]"]
	now_playing -> menu [label="advance [is_going_to_menu]"]
	now_playing -> overview [label="advance [is_going_to_overview]"]
	popular -> menu [label="advance [is_going_to_menu]"]
	popular -> overview [label="advance [is_going_to_overview]"]
	top_rated -> menu [label="advance [is_going_to_menu]"]
	top_rated -> overview [label="advance [is_going_to_overview]"]
	upcoming -> menu [label="advance [is_going_to_menu]"]
	upcoming -> overview [label="advance [is_going_to_overview]"]
	overview -> menu [label="advance [is_going_to_menu]"]
	overview -> user [label=go_back]
}
